#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/files/Projects/HACK-Copenhagen/trees-backend/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/mnt/files/Projects/HACK-Copenhagen/trees-backend/node_modules/.pnpm/mime@1.6.0/node_modules:/mnt/files/Projects/HACK-Copenhagen/trees-backend/node_modules/.pnpm/node_modules:/mnt/files/Projects/HACK-Copenhagen/trees-backend/node_modules:/mnt/files/Projects/HACK-Copenhagen/node_modules:/mnt/files/Projects/node_modules:/mnt/files/node_modules:/mnt/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/files/Projects/HACK-Copenhagen/trees-backend/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/mnt/files/Projects/HACK-Copenhagen/trees-backend/node_modules/.pnpm/mime@1.6.0/node_modules:/mnt/files/Projects/HACK-Copenhagen/trees-backend/node_modules/.pnpm/node_modules:/mnt/files/Projects/HACK-Copenhagen/trees-backend/node_modules:/mnt/files/Projects/HACK-Copenhagen/node_modules:/mnt/files/Projects/node_modules:/mnt/files/node_modules:/mnt/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
